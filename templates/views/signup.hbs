<body>
	
	<div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg-01.jpg');">
			<div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">

				<form class="login100-form validate-form" id="signup" method="POST" action="/users">
					<span class="login100-form-title p-b-49">
						Taskia - Signup
					</span>

					<div class="wrap-input100">
						<h3 class="alert_msg"  name="alert" id="alert_danger"></h3>
						<h3 class="alert_msg"  name="alert" id="alert_success"></h3>
					</div>
<!--
                    <div class="wrap-input100">
						<span class="label-input100">Profile Picture (jpeg, jpg and png ONLY)</span> <br>
                        <span class="focus-input100" ></span>
						<input class="input100" type="file" name="profilepic" id="profilepic" placeholder="jpg, jpeg, or png">
						
					</div>
-->
					<div class="wrap-input100 validate-input m-b-23" data-validate = "First Name is required">
						<span class="label-input100">First Name</span>
						<input class="input100" type="text" name="firstname" id="firstname" placeholder="First Name" required>
						<span class="focus-input100" data-symbol="&#xf206;"></span>
					</div>

                    <div class="wrap-input100 validate-input m-b-23" data-validate = "Last Name is required">
						<span class="label-input100">Last Name</span>
						<input class="input100" type="text" name="lastname" id="lastname" placeholder="Last Name" required>
						<span class="focus-input100" data-symbol="&#xf206;"></span>
					</div>

                    <div class="wrap-input100 validate-input m-b-23" data-validate = "Email is required">
						<span class="label-input100">Email</span>
						<input class="input100" name="email" type = "email" id="email" placeholder="Type your email" title= "Valid email is required" required>
						<span class="focus-input100" data-symbol="&#9993;"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Password is required">
						<span class="label-input100">Password (8 characters minimum)</span>
						<input class="input100" type="password" name="password1" id="password1" minglength = "8" placeholder="Type your password" title="Password must be 8 characters minimum" required>
						<span class="focus-input100" data-symbol="&#xf190;"></span>
					</div>

					<div class="wrap-input100 validate-input" data-validate="Password is required">
						<span class="label-input100">Confirm Password</span>
						<input class="input100" type="password" name="password2" id="password2" minglength = "8" placeholder="Confirm your password" title="Password must be 8 characters minimum" required>
						<span class="focus-input100" data-symbol="&#xf190;"></span>
					</div>
				
				
					<br>
					<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<button class="login100-form-btn">
								Signup
							</button>
						</div>
					</div>
				</form>

					<div class="txt1 text-center p-t-54 p-b-20">
						<span>
							Or Have you registered?
						</span>
					</div>
					<br>
					<div class="container-login100-form-btn">
						<!-- Button directing to the Login form -->
						<input type="button" class="btn btn-success btn-lg" value="Login" onclick="window.location.href = 'index'">
					</div>

				
			</div>
		</div>
	</div>
	

	<div id="dropDownSelect1"></div>
	
